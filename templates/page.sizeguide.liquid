ol.inch_radio_btn li{
  color: #000;
  display: inline-block;
  position: relative;
  float: left;
  width: 100%;
  height: auto;
  margin-left: -20px;
  margin-top:-20px;
}

ol.inch_radio_btn li input[type=radio]{
  position: absolute;
  visibility: hidden;
}

ol.inch_radio_btn li label{
  display: block;
  position: relative;
  font-weight: 300;
  font-size: 16px;
  padding: 27px 25px 25px 50px;
  margin: 3px 0;
  height: 30px;
  z-index: 9;
  cursor: pointer;
  -webkit-transition: all 0.25s linear;
}

ol.inch_radio_btn li:hover label{
	color: #000;
}

ol.inch_radio_btn li .check{
  display: block;
  position: absolute;
  border: 2px solid #999;
  border-radius: 100%;
  height: 20px;
  width: 20px;
  top: 30px;
  left: 20px;
	z-index: 5;
	transition: border .25s linear;
	-webkit-transition: border .25s linear;
}

ol.inch_radio_btn li:hover .check {
  border: 2px solid #999;
}

ol.inch_radio_btn li .check::before {
	display: block;
	position: absolute;
	content: '';
	border-radius: 100%;
	height: 12px;
	width: 12px;
	top: 2px;
	left: 2px;
	margin: auto;
	transition: background 0.25s linear;
	-webkit-transition: background 0.25s linear;
}

ol.inch_radio_btn li input[type=radio]:checked ~ .check {
  border: 2px solid #999;
}

ol.inch_radio_btn li input[type=radio]:checked ~ .check::before{
  background: #e7bfbf;
}

ol.inch_radio_btn li input[type=radio]:checked ~ label{
  color: #555;
}

