<div class="container custom-space">
<div class="row">

<div class="col-sm-3">
<div style="margin-top:20px;">

<h4><strong>Help &amp; Info</strong></h4>
<ul class="help_and_info_menu">
  
{% for link in linklists.sidebar-menu.links %} 
  <li><a class="tablinks" href="{{ link.url }}">{{ link.title }}  </a></li>
{% endfor %}
  <script>
$(function() {
  $('ul.help_and_info_menu a[href="/pages/' + location.pathname.split("/")[2] + '"]').addClass('active');
});
  </script>
</ul>
</div>
</div>
  
<div class="col-sm-9">
	<div class="main-wrap">
      <div class="order-status-form">
        <form method="post" action="" accept-charset="UTF-8">
          <div class="title">Order Status</div>
          <p class="sub-head">Enter your order number and billing address postal code to find your order.</p>

          <label>Order number</label>
          <input type="text" class="form-control" name="customer[email]" placeholder="">
          <label>Billing address postal code</label>  
          <input type="text" class="form-control" name="customer[password]" placeholder="">
          <!--<button type="submit" class="btn">Sign In</button>-->
          <button type="submit" class="btn cust-order">submit</button>
          <!-- /form -->            
        </form>
      </div>
	</div>
</div>


</div>
  
</div>


